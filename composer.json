{
    "name": "d3strukt0r/votifier-client",
    "description": "Simple Tool to send vote request to a server which has the Votifier plugin.",
    "type": "library",
    "keywords": [
        "minecraft",
        "vote",
        "votifier"
    ],
    "homepage": "https://d3strukt0r.github.io/Votifier-PHP-Client/",
    "time": "2015-09-06 21:52:00",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuele Vaccari",
            "email": "manuele.vaccari@gmail.com",
            "homepage": "https://github.com/D3strukt0r"
        }
    ],
    "support": {
    	"email": "manuele.vaccari@gmail.com",
    	"source": "https://github.com/D3strukt0r/Votifier-PHP-Client",
    	"docs": "https://github.com/D3strukt0r/Votifier-PHP-Client/wiki"
    },
    "require": {
        "php": ">=5.3.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "D3strukt0r\\VotifierClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D3strukt0r\\VotifierClient\\": "tests/"
        }
    },
    "archive": {
        "exclude":  [
            ".gitignore",
            ".travis.yml",
            "phpunit.xml.dist",
            "/tests",
            "/docs",
            "/bin",
            "/example.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "upload-coveralls": "php-coveralls -v",
        "upload-scrutinizer": [
            "wget https://scrutinizer-ci.com/ocular.phar",
            "php ocular.phar code-coverage:upload --format=php-clover build/logs/clover.xml"
        ]
    }
}
