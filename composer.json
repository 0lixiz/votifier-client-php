{
    "name": "d3strukt0r/votifier-client",
    "description": "Simple Tool to send vote request to a server which has the Votifier plugin.",
    "type": "library",
    "keywords": [
        "minecraft",
        "vote",
        "votifier"
    ],
    "homepage": "https://github.com/D3strukt0r/votifier-client-php",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Manuele Vaccari",
            "email": "manuele.vaccari@gmail.com",
            "homepage": "https://github.com/D3strukt0r",
            "role": "Developer"
        }
    ],
    "support": {
    	"email": "manuele.vaccari@gmail.com",
        "issues": "https://github.com/D3strukt0r/votifier-client-php/issues",
        "wiki": "https://github.com/D3strukt0r/votifier-client-php/wiki",
    	"source": "https://github.com/D3strukt0r/votifier-client-php",
    	"docs": "https://d3strukt0r.github.io/votifier-client-php/api/"
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-hash": "*",
        "ext-date": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^7",
        "php-coveralls/php-coveralls": "^2",
        "scrutinizer/ocular": "^1",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "D3strukt0r\\VotifierClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D3strukt0r\\VotifierClient\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/build",
            "/docs",
            "/tests",
            ".editorconfig",
            ".gitignore",
            ".php_cs.dist",
            ".phpcs.xml",
            ".scrutinizer.yml",
            ".travis.yml",
            "CONTRIBUTING.md",
            "phpunit.xml.dist",
            "README.md"
        ]
    }
}
